{
  "name": "mis-ts-api",
  "version": "1.0.0",
  "description": "node typescript graphql",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc -p ./tsconfig.prod.json",
    "dev": "nodemon --watch ./src ./src/index.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:unit": "jest unit",
    "test:int": "jest int",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexQianjin/mis-ts-api.git"
  },
  "keywords": [
    "Node",
    "Typescript"
  ],
  "author": "Alex Qin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexQianjin/mis-ts-api/issues"
  },
  "homepage": "https://github.com/AlexQianjin/mis-ts-api#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
